{
	"files.associations": {
		"*.css": "tailwindcss"
	},
	"editor.quickSuggestions": {
		"strings": true
	},
	"tailwindCSS.experimental.configFile": "tailwind.config.ts",
	"tailwindCSS.experimental.classRegex": [
		[
			"ui:\\s*{([^)]*)\\s*}",
			"[\"'`]([^\"'`]*).*?[\"'`]"
		],
		[
			"/\\*ui\\*/\\s*{([^;]*)}",
			":\\s*[\"'`]([^\"'`]*).*?[\"'`]"
		]
	],
	"tailwindCSS.classAttributes": [
		"class",
		"className",
		"ngClass",
		"ui"
	],
	"sqltools.connections": [
		{
			"previewLimit": 50,
			"driver": "SQLite",
			"database": "${workspaceFolder:lspd_timeclock}/db.sqlite3",
			"name": "Timeclock"
		}
	]
}